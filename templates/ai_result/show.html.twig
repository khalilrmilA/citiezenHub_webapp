        </tbody>
    </table>

    <a href="{{ path('app_ai_result_index') }}">back to list</a>

    <a href="{{ path('app_ai_result_edit', {'idAiResult': ai_result.idAiResult}) }}">edit</a>

    {{ include('ai_result/_delete_form.html.twig') }}
{% endblock %}
