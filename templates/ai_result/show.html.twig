{% extends 'base.html.twig' %}

{% block title %}AiResult{% endblock %}

{% block body %}
    <h1>AiResult</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdAiResult</th>
                <td>{{ ai_result.idAiResult }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ ai_result.body }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
                <td>{{ ai_result.timestamp ? ai_result.timestamp|date('Y-m-d H:i:s') : '' }}</td>
