{% for i,product in products %}
{#    {{ dump(product) }}#}
    <!-- start single product -->
    <div class="col-5 col-lg-4 col-md-6 col-sm-6 col-12" id="product_at_index_{{ i }}">
        <div class="product-style-one no-overlay">
            <div class="card-thumbnail">
                <a href="../pages/product-details.html">
                    {% if product.images|length > 1 %}
                    <!-- Swiper -->
                    <div class="swiper effect-creative-swiper rounded">
                        <div class="swiper-wrapper">
                            {% for image in product.images %}
                            <div class="swiper-slide">
                                <a href="#"
                                   onclick="document.getElementById('form_details_{{ i }}').submit(); return false;">
                                    <img src="{{ asset(image.path)|default("Not available") }}"
                                         alt="NFT_portfolio"
                                         class="fixedImagesSize4owner"/>
                                </a>
                            </div>
                            {% endfor %}
                        </div>
                        <div class="swiper-pagination"></div>
                    </div>
                    {% else %}
                    <div>
                        <a href="#"
                           onclick="document.getElementById('form_details_{{ i }}').submit(); return false;">
                            <img src="{{ asset(product.images[0].path) }}"
                                 alt="NFT_portfolio"
                                 class="fixedImagesSize4owner"
                            >
                        </a>
                    </div>
                    {% endif %}
                </a>
                <div class="countdown" data-date="2024-4-25" data-time="11:41">
                </div>
                <div class="rbt-quick-action--btns">
                    {% if underverif %}
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Details">
                        <a
                                href="javascript: void(0);"
                                onclick="showModelUnverfied({{ product.idProduct }})"
                                class="yith-wcqv-button"
                                data-product_id="793"
                        >
                            <i class="fa-solid fa-info"></i>
                        </a>
                    </div>
                    {% else %}
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Contact Seller">
                        <a
                                href="#"
                                onclick="document.getElementById('form_update_{{ i }}').submit(); return false;"
                                class="yith-wcqv-button"
                                data-product_id="793"
                        >
                            <i class="fa-solid fa-pen-to-square" style="font-size: 14px"></i>
                        </a>
                    </div>
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Buy"

                    >

                        <a
                                onclick="deleteProduct({{ product.idProduct }},{{ i }},{{ type|json_encode }})"
                                class="yith-wcqv-button"
                        >
                            <i class="fa-regular fa-trash-can" style="font-size: 14px"></i>
                        </a>

                    </div>
                    {% endif %}
                </div>
            </div>
            {% if underverif %}
                <!--{# <div class="aiverif">
